<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template SYSTEM "../../../project-config.dtd">

<template>
  <name>Framework Template</name>
  <settings>
    <options>
      <option name="artifactRules" value="build/%Configuration%/%Framework% =&gt; nunit-framework_%Framework%.zip" />
      <option name="executionTimeoutMin" value="2" />
    </options>
    <parameters>
      <param name="Configuration" value="Release" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="" type="simpleRunner">
        <parameters>
          <param name="script.content" value="./runbuild /p:Configuration=%Configuration% /p:TestOptions=-teamcity /p:Framework=%Framework% &quot;/t:DumpSettings;CleanAll;Build;Test&quot;" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Framework_VCS_Root" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <cleanup />
  </settings>
</template>

